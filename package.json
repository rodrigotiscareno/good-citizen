{
  "name": "node-react-app",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "install-all": "npm install --legacy-peer-deps && cd client && npm install --legacy-peer-deps && cd ../server && npm install --legacy-peer-deps",
    "client": "cd client && yarn start",
    "server": "cd server && cd src && nodemon --delay 500ms server.ts",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "start": "cd server && cd build && cd server && node server.js",
    "prisma": "cd server && npx prisma generate",
    "client-build": "cd client && npm run build",
    "server-build": "cd server && npm run build",
    "run-server": "cd server && cd build && cd server && node server.js",
    "heroku-postbuild": "npm run client-build",
    "tsc": "./node_modules/typescript/bin/tsc"
  },
